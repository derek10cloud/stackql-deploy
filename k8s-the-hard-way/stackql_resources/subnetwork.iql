/*+ create */
INSERT INTO google.compute.subnetworks
(
 project,
 region,
 data__name,
 data__network,
 data__ipCidrRange,
 data__privateIpGoogleAccess
) 
SELECT
'{{ project }}',
'{{ region }}',
'{{ subnet_name }}',
'{{ vpc_link }}',
'{{ ip_cidr_range }}',
true

/*+ delete */
DELETE FROM google.compute.subnetworks
WHERE subnetwork = '{{ subnet_name }}' AND project = '{{ project }}'
AND region = '{{ region }}'
