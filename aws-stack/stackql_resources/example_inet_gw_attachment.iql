/*+ create */
INSERT INTO aws.ec2.vpc_gateway_attachments (
 InternetGatewayId,
 VpcId,
 region
)
SELECT 
 '{{ internet_gateway_id }}',
 '{{ vpc_id }}',
 '{{ region }}';

/*+ delete */
DELETE FROM aws.ec2.vpc_gateway_attachments
WHERE data__Identifier = 'IGW|{{ vpc_id }}'
AND region = '{{ region }}';