/*+ create */
INSERT INTO aws.ec2.route_tables (
 Tags,
 VpcId,
 region
)
SELECT 
 '{{ route_table_tags | merge_lists(global_tags) }}',
 '{{ vpc_id }}',
 '{{ region }}';

/*+ delete */
DELETE FROM aws.ec2.route_tables
WHERE data__Identifier = '{{ route_table_id }}'
AND region = '{{ region }}';